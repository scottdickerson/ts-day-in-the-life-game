---
import Layout from '../layouts/Layout.astro'
import { GameUI } from '../components/GameUI'
import { DinosaurTypeEnum } from '@/components/utils'

const pageTitle = 'A Day in the Life of a Protohadros'
const gameDataUrl = '/data/Protes.json'
const dinosaurImage = '/protos.png'
const dinosaurName = 'Protohadros'
const dinosaurType = DinosaurTypeEnum.Protos
---

<Layout title={pageTitle} image={dinosaurImage} dinosaurName={dinosaurName}>
    <GameUI client:load gameDataUrl={gameDataUrl} dinosaurType={dinosaurType} />
</Layout>
