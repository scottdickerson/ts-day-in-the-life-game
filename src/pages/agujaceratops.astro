---
import Layout from '../layouts/Layout.astro'
import { GameUI } from '../components/GameUI'
import { DinosaurTypeEnum } from '@/components/utils'

const pageTitle = 'A Day in the Life of an Agujaceratops'
const gameDataUrl = '/data/Agujaceratops.json'
const dinosaurImage = '/SelectScreenAssets/Aguja.png'
const dinosaurName = 'Agujaceratops'
const dinosaurType = DinosaurTypeEnum.Aguja
---

<Layout title={pageTitle} image={dinosaurImage} dinosaurName={dinosaurName}>
    <GameUI client:load gameDataUrl={gameDataUrl} dinosaurType={dinosaurType} />
</Layout>
