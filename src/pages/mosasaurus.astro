---
import Layout from '../layouts/Layout.astro'
import { GameUI } from '../components/GameUI'

const pageTitle = 'A Day in the Life of a Mosasaurus'
const gameDataUrl = '/data/Mosasaurus.json'
const dinosaurImage = '/Mosa.png'
const dinosaurName = 'Mosasaurus'
---

<Layout title={pageTitle} image={dinosaurImage} dinosaurName={dinosaurName}>
    <GameUI client:load gameDataUrl={gameDataUrl} dinosaurType="Mosa" />
</Layout>
