---
import Layout from '../layouts/Layout.astro'
import { GameUI } from '../components/GameUI'
import { DinosaurTypeEnum } from '@/components/utils'

const pageTitle = 'A Day in the Life of a Tyrannosaurus'
const gameDataUrl = '/data/Tyrannosaurus.json'
const dinosaurImage = '/Tyranno.png'
const dinosaurName = 'Tyrannosaurus'
const dinosaurType = DinosaurTypeEnum.Tyranno
---

<Layout title={pageTitle} image={dinosaurImage} dinosaurName={dinosaurName}>
    <GameUI client:load gameDataUrl={gameDataUrl} dinosaurType={dinosaurType} />
</Layout>
